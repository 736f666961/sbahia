		<script type="text/javascript">
			function showDialog() {
				var dialog = document.querySelector('.dialog');
				
				if (dialog.style.display === "block") {
					dialog.style.display = "none";
				}else {
					dialog.style.display = "block";
				}
			}
		</script>
